<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.qx.www.shuang_la_master.activity.YinYuanActivity">

    <include
        android:id="@+id/id_basewebview_toolbar"
        layout="@layout/toolbar"/>

    <com.rey.material.widget.ProgressView
        android:id="@+id/progress_pv_linear_determinate"
        style="@style/LightDeterminateLinearProgressView"
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:layout_below="@id/id_basewebview_toolbar"
        android:layout_gravity="center"
        android:visibility="gone"/>

    <com.qx.www.shuang_la_master.ui.ProgressWebView
        android:id="@+id/id_progwebview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/progress_pv_linear_determinate"
        ></com.qx.www.shuang_la_master.ui.ProgressWebView>

</RelativeLayout>
